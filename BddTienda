CREATE DATABASE [BDTIENDA]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'BDTIENDA', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL15.MSSQLSERVER\MSSQL\DATA\BDTIENDA.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
